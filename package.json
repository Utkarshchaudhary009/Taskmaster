{
  "name": "taskmaster",
  "version": "1.1.0",
  "private": true,
  "description": "High-performance MCP-enabled CLI agent",
  "main": "src/index.ts",
  "type": "module",
  "bin": {
    "tm": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun run build.ts",
    "build:standalone": "bun run build.ts --standalone",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx eslint src/",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@ai-sdk/mcp": "latest",
    "@ai-sdk/openai-compatible": "^2.0.18",
    "ai": "latest",
    "ai-sdk-provider-gemini-cli": "latest",
    "bash-tool": "^1.3.9",
    "prompt-sync": "^4.2.0",
    "tree-sitter-bash": "^0.25.1",
    "web-tree-sitter": "^0.26.3",
    "zhipu-ai-provider": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest"
  }
}